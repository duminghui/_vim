"my vimrc{
set scrolloff=5
set showcmd
set showmode
set history=100
set hlsearch
"查找短语时显示部分区配
set incsearch
set wrapscan
set number
set ruler
set matchpairs=(:),{:},[:],<:>,?:\:
"set cursorline
set relativenumber
syntax on
set shiftwidth=4
set softtabstop=4
"自动缩进
"set autoindent
"显示TAB键及空字符
"set list
"自定义TAB键及空字符显示样式
set listchars=tab:>-,trail:-
"set background=dark
"设置寄存器为系统粘贴板
set clipboard=unnamed
"}
